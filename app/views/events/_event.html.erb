<tr class="<%= cycle('list_line_odd', 'list_line_even') %>">
  <td class='td_left short-size-td'>
  	<%= event.event_date.to_formatted_s(:long) %>  	
  </td>
  <td class='td_holder mm-size-td  module-link'>
  	<%= link_to event.event_types_name, edit_event_path(event) %>  	
  </td>
  <td class='td_holder  long-size-td'>
  	<%= event.description %>  	
  </td>
  <td class='td_holder short-size-td text-center btn_holder'>
    	<%= render partial: 'shared/xlsx_download_grid', 
			locals:{ download_path: all_people_event_path(event.id, format: "xlsx")} %>
  </td>
</tr>