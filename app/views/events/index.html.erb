<% provide(:title, t('events.index.list_of_events')) %>
<h1>
	<span  class="header_title"><%=  t('events.index.list_of_events') %></span>
	<%= link_to t(:new), new_event_path, class: "link_header btn btn-small" %>
</h1>
<%= will_paginate %>
<table>
  <thead>
    <tr>
      <th><%=t('events.form.event_date')%></th>
      <th><%=t(:name)%></th>
      <th><%=t(:description)%></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody  class="tbody_events">
  	<%= render @events %>
  </tbody>
</table>